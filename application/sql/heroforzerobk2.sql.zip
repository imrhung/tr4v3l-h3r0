CREATE PROCEDURE `sp_saveGame`(IN con int, in score int)
BEGIN
	DECLARE tmp INT;
	SELECT Points INTO @tmp FROM userapplication WHERE UserId = con;
	SET @tmp = @tmp + score;
	UPDATE userapplication SET Points = @tmp WHERE UserId = con;
	SELECT Points FROM userapplication WHERE UserId = con;
END ;;
